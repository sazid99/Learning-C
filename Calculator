#include <stdio.h>
int main(){

double num1,num2;
char op;

printf("Enter two numbers: ");
scanf("%lf \t %lf",&num1,&num2);
printf("\n");
printf("Enter the operator[+,-,*,/]");
scanf("%c",op);
printf("\n");

switch(op)
{
case '+':
{
printf("%lf + %lf = %lf",num1, num2, num1+num2);
}
case '-':
{
printf("%lf - %lf = %lf",num1, num2, num1-num2);
}
case '*':
{
printf("%lf * %lf = %lf",num1, num2, num1*num2);
}
case '/':
{
printf("%lf / %lf = %lf",num1, num2, num1/num2);
}

}

}
