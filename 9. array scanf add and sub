#include <stdio.h>
void main(){
    int number[100],i,n;
    float sum,avg;
    scanf("%d",&n);
    
    for(i=0; i<n;i++)
    {
        scanf("%d",&number[i]);
    }
    
    for(i=0;i<n;i++)
    {
        sum+= number[i];
        avg= sum/n;
    }
    printf("%f\n",sum);
     printf("%.2f",avg);
}
