#include <stdio.h> 
int main() {
    unsigned int counter ;
    int grade ;
    int total ; 
    int average ;
    total = 0 ;
    counter = 0 ;
    //starting condition 
    while ( counter <= 10 ) {
        printf("%s","Enter the grade: ");
        scanf("%d",&grade);
        total = total + grade ;
        counter = counter + 1 ;
    }
    //end of condition
    average = total / 10 ;
    
    printf("Class average is: %d \n", average);
    
}
