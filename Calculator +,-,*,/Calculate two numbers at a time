#include <stdio.h>
int main(){

double num1,num2;
char op;

printf("Enter the operator[+,-,*,/] : ");
scanf("%c",&op);
printf("\n");

printf("Enter 1st number: ");
scanf("%lf",&num1);
printf("\n");
printf("Enter 2nd number: ");
scanf("%lf",&num2);
printf("\n");

switch(op)
{
case '+':
{
printf("%lf + %lf = %lf",num1, num2, num1+num2);
break;
}
case '-':
{
printf("%lf - %lf = %lf",num1, num2, num1-num2);
break;
}
case '*':
{
printf("%lf * %lf = %lf",num1, num2, num1*num2);
break;
}
case '/':
{
printf("%lf / %lf = %lf",num1, num2, num1/num2);
break;
}

}

}
